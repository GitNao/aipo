## ---------------------------------------------------------------------------
#set ($indicator_id = "indicator-list-")
## ---------------------------------------------------------------------------
#AUIwideAreaHeader()
#if($result.ProjectCount == 0)
<div class="message">プロジェクトが登録されていません。</div>
#else
#AUISummaryDiv()
#AUIajaxpageview('ProjectScreen' $result)
#AUISummaryDivfooter()
<table class="auiRowTable wide">
<colgroup style=""></colgroup>
<colgroup style=""></colgroup>
<colgroup style="width:8em;" />
<tbody>
#foreach ($record in $result.List)
	<tr>
	#ALtdcellheader()
	<a href="$!utils.escapeXML($!jslink.getPortletById($!portlet.ID).addQueryData('action','controls.Maximize').addQueryData('filter',$record.ProjectId).addQueryData('filtertype','project_id'))">$!record.ProjectName</a>
	#ALtdcellfooter()
	#ALtdcellheader()
	$!{record.UpdateDate.Year}/$!{record.UpdateDate.Month}/$!{record.UpdateDate.Day}
	#ALtdcellfooter()
	#ALtdcellheader()
	<a href="$!utils.escapeXML($!jslink.getPortletById($!portlet.ID).addQueryData('action','controls.Maximize').addQueryData('filter',$record.ProjectId).addQueryData('filtertype','project_id').addQueryData('gantt','t'))">ガントチャート</a>
	#ALtdcellfooter()
	</tr>
#end
</table>
#end
#AUIcontentsAreaFooter()
