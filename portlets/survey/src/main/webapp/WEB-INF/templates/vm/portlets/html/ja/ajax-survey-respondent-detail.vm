<div class="borderAuiHead">
<div id="messageDiv"></div>
#ALdialogheader($result)
    <div class="auiListWrapper">
		<ul class="userList">
#foreach ($record in $list)
	<li>
		<div class="content">
			<div class="avatar">
#if(${record.hasPhoto()})
				<td class="caption"><img style="border-radius: 3px;" src="$!utils.escapeXML($jslink.getTemplate("FileuploadFacePhotoScreen").addQueryData("uid", ${record.UserId}).addQueryData("t", ${record.PhotoModified}))" alt="${record.UserName}" width="50" height="50" /></td>
#else
      <td class="caption"><img style="border-radius: 3px;" src="images/common/avatar_default3.png" alt="${record.UserName}" width="50" height="50" /></td>
#end
</div>
<div class="name">
#ALtditem($record.AliasName)
</div>
</div>
</li>
#end
</ul>
#ALdialogfooter()
